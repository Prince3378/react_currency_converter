{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "@mate-academy/eslint-config",
  "overrides": [
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "react",
      "@typescript-eslint"
  ],
  "rules": {
      "indent": ["error", 2],
      "jsx-quotes":["error", "prefer-double"]
  }
}
